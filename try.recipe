[my_cron]
type = cron
crontab_schedule = */1 * * * *

[scrivi_mastro]
type=log
input = my_cron
text={time} - MASTRO


[my_timer]
type=timer
delay=1

[my_timer2]
type=timer
delay=2

[my_timer4]
type=timer
delay=4

[scrivi]
type=log
input=my_timer
text={date} - {time}

[scrivi2]
type=log
input=my_timer2
text={date} - {time} --- 2

[scrivi4]
type=log
input=my_timer4
text={date} - {time} --- 4

